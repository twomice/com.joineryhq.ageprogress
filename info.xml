<?xml version="1.0"?>
<extension key="com.joineryhq.ageprogress" type="module">
  <file>ageprogress</file>
  <name>Sub-type progress by age</name>
  <description>Automatically progresses contacts through a series of sub-types based on age, according to configurable rules</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.ageprogress/</url>
    <url desc="Documentation">https://twomice.github.io/com.joineryhq.ageprogress/</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.ageprogress/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-20</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Initial development sponsored by Nameless Network.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Ageprogress</namespace>
  </civix>
</extension>
